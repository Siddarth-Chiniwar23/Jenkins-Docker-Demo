pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                git https://github.com/Siddarth-Chiniwar23/Jenkins-Docker-Demo.git
            }
        }
        stage('Install') {
            steps {
                echo 'npm install '
            }
        }
        stage('Test') {
            steps {
                echo 'Running testing....'
            }
        }
    }
}